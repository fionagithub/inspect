<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jkinfo.laputaApp" version="0.5.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LaputaApp</name>
    <description>
        A laputa cloud app
    </description>
    <author email="mxj@jkinfo.com.cn" href="http://www.jkinfo.com.cn">
        JKR3 Team @ JKINFO
    </author>
    <content src="index.html" />
    <access origin="m.laputacloud.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="webviewbounce" value="false" />
        <preference name="UIWebViewBounce" value="false" />
        <preference name="StatusBarOverlaysWebView" value="true" />
        <preference name="StatusBarCropsWebview" value="false" />
    </platform>
    <platform name="ios">
        <icon height="40" src="res/icon/ios/icon-20@2x-ipad.png" width="40" />
